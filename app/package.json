{
  "name": "upload-pdf-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "test:integration": "jest --forceExit test/local-development.test",
    "test:integration:extended": "cross-env IS_EXTENDED=true jest --forceExit test/local-development.test",
    "test:integration:extended:ci-reporter": "cross-env IS_EXTENDED=true jest --forceExit || node test/utils/image-reporter-standalone.js",
    "login": "clasp login",
    "clone": "rimraf .clasp.json && mkdirp dist && clasp clone 1zQgKiwaw0bf3aXaYz1JGlreExYQqPtCfrHfeU8G2iatD2oRrG9zkwkDM --rootDir ./dist && mv ./dist/.clasp.json ./.clasp.json && rimraf dist",
    "setup": "rimraf .clasp.json && mkdirp dist && clasp create --type slides --title \"uploadApplication\" --rootDir ./dist && mv ./dist/.clasp.json ./.clasp.json && rimraf dist",
    "open": "clasp open --addon",
    "push": "clasp push",
    "pull": "clasp pull",
    "create-certs": "mkdirp certs && mkcert -key-file ./certs/key.pem -cert-file ./certs/cert.pem localhost 127.0.0.1",
    "build:dev": "tsc && vite build --mode development",
    "build": "tsc && vite build --mode production",
    "deploy:dev": "npm run build:dev && npm run push",
    "deploy": "npm run build && npm run push",
    "start": "npm run deploy:dev && npm run dev"
  },
  "dependencies": {
    "@babel/preset-env": "^7.25.7",
    "clasp": "^1.0.0",
    "gas-client": "^1.1.1",
    "mobx": "^6.13.3",
    "mobx-react-lite": "^4.0.7",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "sweetalert2": "^11.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@types/node": "^20",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "gas-types-detailed": "^1.1.2",
    "lucide-react": "^0.412.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vite": "^5.3.4",
    "vite-plugin-babel": "^1.2.0",
    "vite-plugin-singlefile": "^2.0.2",
    "vite-plugin-static-copy": "^1.0.6"
  }
}
